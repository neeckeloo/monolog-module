{
    "name": "neeckeloo/monolog-module",
    "description": "Monolog integration into Laminas",
    "type": "library",
    "keywords": [
        "laminas", "monolog", "logger", "log"
    ],
    "license": "MIT",
    "homepage": "https://github.com/neeckeloo/MonologModule",
    "authors": [
        {
            "name": "Nicolas Eeckeloo",
            "email": "neeckeloo@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "monolog/monolog": "^1.11 || ^2.0",
        "laminas/laminas-servicemanager": "^2.7.6 || ^3.0.3"
    },
    "require-dev": {
        "graylog2/gelf-php": "~1.1",
        "phpunit/phpunit": "^7.5.20 || ^8.5.8",
        "satooshi/php-coveralls": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "MonologModule\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MonologModuleTest\\": "tests/"
        }
    },
    "extra": {
        "zf": {
            "module": "MonologModule\\Module"
        }
    }
}
